  <data name="InsertRecipe" xml:space="preserve">
    <value>INSERT INTO [dbo].[recipe]
           (FK_authorId
           ,name
           ,description
           ,FK_recipetype
           ,Ingredients
           ,preptime
           ,cooktime
           ,steps
           ,shown
           ,image)
     VALUES
           (@authorid
           ,@name
           ,@desc
           ,@recipetype
           ,@Ingredients
           ,@preptime
           ,@cooktime
           ,@steps
           , 1
           ,@image); SELECT SCOPE_IDENTITY();</value>
